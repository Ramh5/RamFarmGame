<?xml version="1.0" encoding="UTF-8"?>
<?import java.net.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<BorderPane prefHeight="1000.0" prefWidth="1300.0"
	xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
	fx:controller="homier.farmGame.controller.Engine">
	<center>
		<StackPane prefHeight="150.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<GridPane fx:id="gameGridPane" alignment="CENTER">

				</GridPane>
				<Canvas height="200.0" mouseTransparent="true" opacity="0.0"
					width="200.0" />
			</children>
		</StackPane>
	</center>
	<top>
		<Label fx:id="clockLabel" text="Label" BorderPane.alignment="CENTER" />
	</top>
	<left>
		<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
			<children>
				<Label fx:id="pauseLabel" alignment="CENTER"/>
				<ToggleButton fx:id="pauseButton" alignment="CENTER"  onAction="#pauseButtonAction" />
			</children>
		</VBox>
	</left>
</BorderPane>
